{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "shop-web-admin",
  "version": "0.3.9",
  "identifier": "io.github.caolib",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:14200",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "网上商店-管理员端",
        "width": 1200,
        "height": 750,
        "minHeight": 550,
        "minWidth": 1000,
        "label": "main",
        "fullscreen": false,
        "resizable": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "nsis",
      "msi"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "allowDowngrades": true,
      "certificateThumbprint": null,
      "digestAlgorithm": null,
      "nsis": {
        "displayLanguageSelector": true,
        "installerIcon": "icons/icon.ico",
        "languages": [
          "SimpChinese",
          "English"
        ],
        "installMode": "perMachine"
      },
      "signCommand": null,
      "timestampUrl": null,
      "tsp": false,
      "webviewInstallMode": {
        "silent": true,
        "type": "downloadBootstrapper"
      },
      "wix": {
        "language": [
          "zh-CN"
        ]
      }
    }
  }
}